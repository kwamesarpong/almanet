<div class="container">
                <div class="imp_card mdl-card mdl-shadow--2dp">
                    <div class="mdl-card__title">
                        <h2 class="mdl-card__title-text">Welcome</h2>
                    </div>
                    <div align="center">
                        <br />
                        <div class="mdl-grid">
                 <div class="mdl-cell mdl-cell--8-col">Upload your existing member data. (CSV or Excel format)</div>
                            <div class="mdl-cell mdl-cell--4-col">

                                <!-- import_organisation_membershipdata_path(current_organisation.id) -->
                                <%= form_tag import_membershipdata_path , multipart: true do%>
                                <%= file_field_tag :files %>
                                <%= submit_tag "Import" , :class => "mdl-button mdl-js-button mdl-button--raised mdl-js-ripple-effect mdl-button--accent" %>
                                <% end %>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <br /><br />
            <div class="container" align="center"><!-- Feed from database -->
                <table class="mdl-data-table mdl-js-data-table mdl-data-table--selectable mdl-shadow--2dp">
                    <thead>
                        <tr>
                            <th class="mdl-data-table__cell--non-numeric">First Name</th>
                            <th>Last Name</th>
                            <th>Organization</th>
                            <th>Phone Number</th>
                            <th>E-Mail</th>
                            <th>Work Place</th>
                            <th>Year Completed</th>
                        </tr>
                    </thead>
                    <tbody>
                       <% for member in @members do %>
                           <tr>
                            <td class="mdl-data-table__cell--non-numeric"> <%= member.FirstName %> </td>
                            <td><%= member.LastName%></td>
                            <td><%= member.Organisation%></td>
                            <td><%= member.PhoneNumber%></td>
                            <td><%= member.Email%></td>
                            <td><%= member.WorkPlace %></td>
                            <td><%= member.YearCompleted %></td>
                        </tr>
                       <%  end  %>


                    </tbody>
                </table>
            </div>
