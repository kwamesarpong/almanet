            <div class="container">
                <div class="event_card mdl-shadow--2dp">
                    <div class="mdl-grid">
                        <div class="mdl-cell mdl-cell--6-col">

                        </div>
                        <div class="mdl-cell mdl-cell--6-col" align="center">
                            <%= link_to "Add Event" , new_event_path , :class => "mdl-button mdl-js-button mdl-button--raised mdl-js-ripple-effect mdl-button--accent" %>
                        </div>
                    </div>

                </div>
                <br />
